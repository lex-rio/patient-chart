  "homepage": "https://github.com/lex-rio/patient-chart#readme",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/sequelize db:migrate && node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "db-reload": "node_modules/.bin/sequelize db:migrate && node_modules/.bin/sequelize db:seed:all"
  },
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~4.1.0",
    "express": "~4.16.0",
    "http-errors": "~1.7.1",
    "morgan": "~1.9.0",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.3.0",
    "sqlite3": "^4.0.3",
    "twig": "^1.12.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.5",
    "supertest": "^3.3.0"
  }
